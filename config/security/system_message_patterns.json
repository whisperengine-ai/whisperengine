  "filtered_content_patterns": {
    "description": "Patterns that indicate already filtered content is leaking through",
    "patterns": [
      {
        "name": "system_information_filtered_brackets",
        "pattern": "\\[SYSTEM_INFORMATION_FILTERED\\]",
        "description": "Bracketed filtered content marker"
      },
      {
        "name": "system_information_filtered_literal",
        "pattern": "SYSTEM_INFORMATION_FILTERED",
        "description": "Literal filtered content marker"
      },
      {
        "name": "context_filtered_brackets",
        "pattern": "\\[CONTEXT_FILTERED\\]",
        "description": "Bracketed context filtered marker"
      },
      {
        "name": "json_filtered_field",
        "pattern": "\\{\\s*\\[SYSTEM_INFORMATION_FILTERED\\]\\s*:",
        "description": "JSON field with filtered marker as key"
      },
      {
        "name": "json_mode_value",
        "pattern": "\"[A-Z_]+\"\\s*:\\s*\"[^\"]*Mode[^\"]*\"",
        "description": "JSON field with mode-related value"
      },
      {
        "name": "json_intelligence_value",
        "pattern": "\"[A-Z_]+\"\\s*:\\s*\"[^\"]*Intelligence[^\"]*\"", 
        "description": "JSON field with intelligence-related value"
      },
      {
        "name": "json_filtered_content_structure",
        "pattern": "\\{\\s*\\[SYSTEM_INFORMATION_FILTERED\\]\\s*:\\s*\"[^\"]*\"",
        "description": "JSON object with filtered content as field"
      }
    ]
  }
