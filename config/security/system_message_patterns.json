  "json_debug_patterns": {
    "description": "JSON structure patterns that indicate debug information leakage",
    "patterns": [
      {
        "name": "json_memory_network_context",
        "pattern": "\"memory_network_context\"",
        "description": "JSON memory network context field"
      },
      {
