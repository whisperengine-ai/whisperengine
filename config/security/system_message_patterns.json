{
  "sensitive_patterns": {
    "description": "Patterns that indicate system prompt leakage or sensitive information disclosure",
    "patterns": [
      {
        "name": "system_prompt_structure",
        "pattern": "##Archive.*?##Identity",
        "description": "System prompt section markers"
      },
      {
        "name": "system_prompt_sections", 
        "pattern": "##Personality.*?##Relationships",
        "description": "System prompt personality sections"
      },
      {
        "name": "memory_sections",
        "pattern": "##Memory.*?##Behavior", 
        "description": "System prompt memory sections"
      },
      {
        "name": "role_content_structure",
        "pattern": "role.*?system.*?content",
        "description": "Message role/content structure indicators"
      },
      {
        "name": "default_system_prompt",
        "pattern": "DEFAULT_SYSTEM_PROMPT",
        "description": "Default system prompt variable name"
      },
      {
        "name": "conversation_context_append",
        "pattern": "conversation_context\\.append",
        "description": "Internal context manipulation code"
      },
      {
        "name": "memory_manager_calls",
        "pattern": "memory_manager\\.",
        "description": "Memory manager internal calls"
      },
      {
        "name": "chromadb_references",
        "pattern": "ChromaDB",
        "description": "Database system references"
      },
      {
        "name": "llm_server_references",
        "pattern": "LLM.*?server",
        "description": "LLM server implementation details"
      },
      {
        "name": "discord_bot_token",
        "pattern": "Discord.*?bot.*?token",
        "description": "Authentication token references"
      },
      {
        "name": "user_id_patterns",
        "pattern": "user_id.*?[0-9]+",
        "description": "User ID with actual values"
      },
      {
        "name": "emotion_context_relationship",
        "pattern": "emotion_context.*?relationship.*?level",
        "description": "Internal emotion context data"
      },
      {
        "name": "interaction_counts",
        "pattern": "interaction_count.*?[0-9]+",
        "description": "User interaction statistics"
      },
      {
        "name": "pre_analyzed_emotion",
        "pattern": "pre_analyzed_emotion_data",
        "description": "Internal emotion analysis data"
      },
      {
        "name": "debug_mode_enabled",
        "pattern": "debug.*?mode.*?enabled",
        "description": "Debug mode indicators"
      },
      {
        "name": "admin_permissions",
        "pattern": "admin.*?permissions",
        "description": "Administrative permission data"
      },
      {
        "name": "conversation_cache",
        "pattern": "conversation.*?cache",
        "description": "Internal caching mechanisms"
      },
      {
        "name": "bootstrap_limit",
        "pattern": "bootstrap.*?limit",
        "description": "System bootstrap configuration"
      }
    ]
  },
  
