        "name": "json_context_structure",
        "pattern": "\\{[\\s\\n]*\"[a-z_]+_context\"",
        "description": "JSON object with context field"
      },
      {
        "name": "json_uppercase_context",
        "pattern": "\"[A-Z_]+_CONTEXT\"",
        "description": "JSON field with uppercase context variable name"
      },
      {
        "name": "json_context_field_pattern",
        "pattern": "CONTEXT.*?:.*?\"[^\"]*\"",
        "description": "JSON context field with value"
      }
    ]
